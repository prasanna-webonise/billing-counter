<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="insertCategories" author="Prasanna">
        <insert tableName="category">
            <column name="category_type" value="A"/>
            <column name="tax_percent" value="10"/>
        </insert>
        <insert tableName="category">
            <column name="category_type" value="B"/>
            <column name="tax_percent" value="20"/>
        </insert>
        <insert tableName="category">
            <column name="category_type" value="C"/>
            <column name="tax_percent" value="0"/>
        </insert>
    </changeSet>

</databaseChangeLog>
